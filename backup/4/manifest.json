	
	{
	
		"manifest_version": 2,
	
		"name": "Time Tracker",
		"description": "Time Tracker for zipbooks.com",
		"version": "1.1",
		
		"icons": {
			"128": "img/zipbooks.png"
		},
		
		"browser_action": {
			"default_icon": "img/zipbooks.png",
			"default_title": "Open zipbooks Time Tracker",
			"default_popup": "popup/popup.html"
		},
		
		"background": {
			
			"page": "background/background.html"
			
		},
		
		"permissions": [
			"https://api.zipbooks.com/*",
			"storage"
		]

	}
	